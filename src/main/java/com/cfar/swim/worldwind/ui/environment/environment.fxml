<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<TitledPane animated="false" prefHeight="200.0" prefWidth="800.0" text="Environment" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cfar.swim.worldwind.ui.environment.EnvironmentPresenter">
  <content>
      <TreeView fx:id="environment">
         <contextMenu>
            <ContextMenu>
              <items>
                <MenuItem mnemonicParsing="false" onAction="#refineEnvironment" text="Refine" />
                  <MenuItem mnemonicParsing="false" onAction="#coarsenEnvironment" text="Coarsen" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TreeView>
  </content>
</TitledPane>

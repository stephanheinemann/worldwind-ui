<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cfar.swim.worldwind.ui.setup.SetupPresenter">
  <tabs>
    <Tab text="Aircraft">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="600.0" />
      </content>
    </Tab>
    <Tab fx:id="environmentTab" text="Environment">
      <content>
            <VBox prefHeight="400.0" prefWidth="600.0">
               <children>
                  <HBox prefHeight="26.0" prefWidth="580.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="140.0" text="Environment Type" />
                        <ComboBox fx:id="environment" prefHeight="25.0" prefWidth="440.0" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <ScrollPane fx:id="envPropertiesPane" prefHeight="324.0" prefWidth="600.0" />
               </children>
            </VBox>
      </content>
    </Tab>
      <Tab text="SWIM">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="600.0" />
        </content>
      </Tab>
      <Tab text="Planner">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="600.0" />
        </content>
      </Tab>
  </tabs>
</TabPane>
